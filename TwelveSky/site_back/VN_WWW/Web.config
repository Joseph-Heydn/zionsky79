	<connectionStrings>
		<!-- 공통 -->
		<add name="WWW_DATA" connectionString="Data Source=127.0.0.1,9174;Initial Catalog=Twelvesky2_DB;User ID=sawebuser;Password=apclslzhvm11!;Network Library=DBMSSOCN;" providerName="System.Data.SqlClient"/>
		<!-- 필리핀 -->
